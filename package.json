{
  "name": "stylecast",
  "description": "A fast and lightweight inline CSS style parser for JavaScript and TypeScript",
  "version": "0.0.3",
  "packageManager": "pnpm@10.15.0",
  "license": "MIT",
  "type": "module",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "bench": "tsx benchmark/benchmark.ts",
    "build": "tsdown",
    "test": "vitest",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arikchakma/stylecast.git"
  },
  "author": "Arik Chakma <arikchangma@gmail.com>",
  "keywords": [
    "stylecast",
    "inline",
    "css",
    "parser",
    "css-parser",
    "inline-css-parser"
  ],
  "devDependencies": {
    "bumpp": "^10.3.1",
    "inline-style-parser": "^0.2.6",
    "mitata": "^1.0.34",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsdown": "^0.16.4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
